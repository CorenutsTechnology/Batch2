package com.crnts.threads;

public class SingleThreads extends Thread {
	
	public SingleThreads()
	{
		super();
	}
	
	public SingleThreads(String name)
	{
		super(name);
	}

	@Override
	public void run() {
		System.out.println(Thread.currentThread().getName());
		
		Thread.sleep(50000000);
	}
	
 
}
