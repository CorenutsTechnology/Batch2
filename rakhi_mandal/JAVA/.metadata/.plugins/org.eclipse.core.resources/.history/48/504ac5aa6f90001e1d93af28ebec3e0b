package com.crnts.solid;

public class Billing_S {
	
	Billing billing;
	

	public Billing_S(Billing billing) {
		super();
		this.billing = billing;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
      

	public double calculateTotal() {
        double price = (billing.getQuantity()*billing.getBook().price);
        return price;
}

}
