package com.crnts.thread_2;

public class SlowFaceThread_Test {
  public static void main(String[] args) {
	
}
  public void slow()
  {
	  Thread first =new Thread(new SlowFaceThread());
		try {
			first.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		first.start();
  }
}
