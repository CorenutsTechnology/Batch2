package com.crnts.solid;

public class Billing_S {
	
	Billing billing;
	


	public Billing_S(Book book, int i) {
		// TODO Auto-generated constructor stub
	}


	public double calculateTotal() {
        double price = (billing.getQuantity()*billing.getBook().price);
        return price;
}
	
	 public static void main(String[] args) {
    	 Billing_S billing =new Billing_S(new Book("Java","Herbert",2017,450),2);
    	 billing.calculateTotal();
//    	 billing.printInvoice();
    	 
	}

}
