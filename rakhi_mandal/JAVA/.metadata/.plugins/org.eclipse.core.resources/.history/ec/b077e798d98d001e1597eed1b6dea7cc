package com.crnts.Generics;

public class GenericClass<T> {
     T id;
     T name;
     int number;
   
    public T getId() {
		return id;
	}

	public void setId(T id) {
		this.id = id;
	}

	public T getName() {
		return name;
	}

	public void setName(T name) {
		this.name = name;
	}

	public T genericMethods(T obj)
    {
    return obj;
    
}
	public static void main(String[] args) {
		GenericClass genericClass =new GenericClass();
		System.out.println(genericClass.id);
		System.out.println(genericClass.number);
	}
}
