package com.crnts.List;
import org.apache.commons.lang.StringUtils;

public class Test {
	public static void main(String[] args) {
		String value = " ";
	
		if (StringUtils.isNotEmpty(value))
		System.out.println("Hello World");
		if (StringUtils.isNotBlank(value))
		System.out.println("Hello World");
		}


}
